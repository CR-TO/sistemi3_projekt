{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "Roof solution",
    "required": [
        "roof_type_id",
        "color_id"
    ],
    "additionalProperties": false, 
    "properties": {
        "id": {
            "type": "integer",
            "title": "The id Schema"
        },
        "roof_type_id": {
            "type": "integer",
            "default": 0,
            "title": "roof_type_id "
        },
        "color_id": {
            "type": "integer",
            "title": "The color_id Schema"
        },
        "created_at": {
            "type": "string",
            "title": "The created_at Schema"
        },
        "updated_at": {
            "type": "string",
            "title": "The updated_at Schema"
        },
        "deleted_at": {
            "type": "null",
            "title": "The deleted_at Schema"
        }
    }
}